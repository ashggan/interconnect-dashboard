{
  "extends": "next/core-web-vitals",
  "plugins": ["@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "@typescript-eslint/no-unused-vars": ["warn", { "args": "none" }],
    "import/no-unresolved": [2, { "caseSensitive": false }],
    "import/named": "off",
    "no-console": "off"
  }
}
